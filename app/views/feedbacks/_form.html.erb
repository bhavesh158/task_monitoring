<%= form_for @feedback, :url => {:action => :create} do |f| %>
<% if @user.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

    <ul>
    <% @user.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

  <div class="field">
    <%= f.label :team_name, class:"label" %><br />
    <%= f.text_field :team_name, value: @team.team_name, readonly: :true %>
  </div>
  
  <div class="field">
    <%= f.label :user_name, class:"label" %><br />
    <%= f.text_field :user_name, value: (@user.first_name + " " + @user.last_name), readonly: :true %>
  </div>
  
  <div class="field">
    <%= f.label :content %><br />
    <%= f.text_area :content, size: "3x3" %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
